{
  "name": "browserify-zlib",
  "version": "0.1.4",
  "description": "Full zlib module for browserify",
  "keywords": [
    "zlib",
    "browserify"
  ],
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "pako": "~1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "brfs": "^1.0.1",
    "karma": "^1.1.0",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^1.0.1",
    "karma-mocha-own-reporter": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^3.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "standard": "^6.0.8",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "standard \"src/binding.js\" \"test/**/*.js\"",
    "pretest": "npm run build",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "karma start --single-run=true karma.conf.js",
    "test:node": "mocha test/test-*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Devon Govett <devongovett@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/devongovett/browserify-zlib.git"
  }
}
